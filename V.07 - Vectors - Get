double i, var;    
int s;
string str;

Vector1.Init(1);  

ClearOutput();  

str = ' -th Vector1 value is: ';

for(i = 1 ; i <= 10; i += 1) {
    
   var = Random(5,20);
   var = round(var);
   Vector1.Set(i,var);
} 

s = Vector1.Size(); !Get the value of a vector        

for (i = 1; i <= s; i += 1){
      
    var = Vector1.Get(i); !Get a i th value
    printf('%d%s %d', i, str, var);
   
}
