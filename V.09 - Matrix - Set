int i, j, a, b, NmOfRow, NmOfCol;
double var;
int s;
string str1, str2, str3;

Matrix1.Init(1,1);


a = 5;
b = 10;

str1 = 'Matrix1 have';
str2 = 'rows, and';
str3 = 'columns!';

for (i = 1; i <= a; i += 1){
      
      for (j = 1; j <= b; j += 1){
       
       var = Random(1,10);
       var = round(var);
       Matrix1.Set(i,j, var); !Set a value from a matrice
      
      }

}  

    NmOfRow = Matrix1.NRow(); !Get the # of rows
    NmOfCol = Matrix1.NCol(); !Get the # of columns
     
     
    printf('%s %d %s %d %s', str1, NmOfRow, str2, NmOfCol, str3);
    
    
! If you run the command without creating matrix set, will not work
! Apply the following:
! 1. Click Contents button at the right side of window
! 2. Click on New Object
! 3. Select DPL Command and more
! 4. Select Matrix (IntMat) from selection options
! 5. Save and Execute the script
! 6. Too see your results -> 1. Click the script from script table
! 7. Too see your results -> 2. Click Contents
! 8. Too see your results -> 3. Click Matrix1. 
! You will see the results of Matrix. 
